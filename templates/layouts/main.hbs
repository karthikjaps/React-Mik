<!DOCTYPE html>
<html {{{ htmlAttributes }}}>

<head>
    {{{ head }}}
    <style type="text/css">{{{data.inlineStyles}}}</style>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="theme-color" content="#fa4a9f" />
    <link rel="manifest" href="/manifest.json" />
    <link href="/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    {{> shared/google-tag-manager }}
</head>

    <body {{{ bodyAttributes }}}>

        {{{body}}}

        {{> shared/async-css }}

        {{#each data.vendorScripts}}
            <script src="{{this}}"></script>
        {{~/each}}
        {{#each data.remoteScripts}}
            <script src="{{this}}" async></script>
        {{~/each}}

        {{> shared/google-tag-manager-noscript }}
    </body>
</html>
